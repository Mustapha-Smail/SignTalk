{
  "name": "signtalk",
  "version": "0.1.0",
  "description": "Application web d’apprentissage de langue des signes française",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "jest --forceExit",
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mustapha-Smail/signtalk-mern.git"
  },
  "keywords": [
    "mern",
    "jest",
    "node",
    "react",
    "mongodb",
    "express"
  ],
  "author": "Mustapha Smail",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Mustapha-Smail/signtalk-mern/issues"
  },
  "homepage": "https://github.com/Mustapha-Smail/signtalk-mern#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "mongoose": "^6.6.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "concurrently": "^7.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^29.1.1",
    "mockgoose": "^8.0.4",
    "nodemon": "^2.0.20",
    "supertest": "^6.2.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
